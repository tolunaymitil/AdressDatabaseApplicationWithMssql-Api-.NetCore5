@{
}
<form>

</form>


@if (string.IsNullOrEmpty(ViewBag.Error) == false)
{
    <span>@ViewBag.Error</span>
    @if (ViewBag.ErrorMessageList.Count > 0)
    {
        @foreach (var item in ViewBag.ErrorMessageList)
        {
            <div>@item</div>
        }
    }

}
else
{
    <span>@ViewBag.Success</span>
}
